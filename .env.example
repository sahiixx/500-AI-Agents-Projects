# ==============================================
# 500-AI-Agents-Projects Environment Variables
# ==============================================
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# ==============================================
# AI/LLM Provider APIs
# ==============================================

# OpenAI API (required for CrewAI and LangChain)
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
OPENAI_ORG_ID=org-your-organization-id  # Optional

# Alternative LLM providers (optional)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
COHERE_API_KEY=your-cohere-api-key
HUGGINGFACE_API_KEY=hf_your-huggingface-token

# ==============================================
# CRM Integrations
# ==============================================

# Google Sheets API
GOOGLE_SHEETS_CREDENTIALS_FILE=/path/to/google-credentials.json
GOOGLE_SHEETS_SPREADSHEET_ID=1abc123_your-spreadsheet-id
GOOGLE_SHEETS_WORKSHEET_NAME=Leads  # Default worksheet name

# Airtable API
AIRTABLE_API_KEY=patyour-airtable-api-key
AIRTABLE_BASE_ID=appYourBaseId123
AIRTABLE_TABLE_NAME=Leads

# ==============================================
# Communication Services
# ==============================================

# Twilio (SMS/WhatsApp)
TWILIO_ACCOUNT_SID=ACyour-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_WHATSAPP_NUMBER=whatsapp:+1234567890

# SendGrid (Email)
SENDGRID_API_KEY=SG.your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@yourdomain.com
SENDGRID_FROM_NAME=Your Company Name

# ==============================================
# n8n Automation
# ==============================================

# n8n Webhook URLs
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-webhook-id
N8N_API_KEY=your-n8n-api-key  # If using authentication

# ==============================================
# Dubai Real Estate Specific
# ==============================================

# Dubai Land Department API (if available)
DUBAI_LAND_DEPT_API_URL=https://api.dubailand.gov.ae/v1
DUBAI_LAND_DEPT_API_KEY=your-dubai-land-dept-api-key

# Property Platforms (if API access available)
PROPERTY_FINDER_API_KEY=your-property-finder-api-key
BAYUT_API_KEY=your-bayut-api-key
DUBIZZLE_API_KEY=your-dubizzle-api-key

# ==============================================
# Web Scraping Configuration
# ==============================================

# Proxy settings (if using proxies for scraping)
HTTP_PROXY=http://your-proxy:8080
HTTPS_PROXY=https://your-proxy:8080

# User agent for scraping
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Scraping rate limits (requests per minute)
SCRAPING_RATE_LIMIT=10

# ==============================================
# Database (if using one)
# ==============================================

# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# MongoDB
MONGODB_URI=mongodb://localhost:27017/ai_agents

# ==============================================
# Application Settings
# ==============================================

# Environment
ENVIRONMENT=development  # development, staging, production

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=/var/log/ai-agents.log

# Application URL
APP_URL=http://localhost:8000

# ==============================================
# Security
# ==============================================

# API Rate Limiting
API_RATE_LIMIT=100  # requests per minute

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# JWT (if using authentication)
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# ==============================================
# Monitoring & Analytics
# ==============================================

# Sentry (Error tracking)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Google Analytics
GA_TRACKING_ID=UA-your-tracking-id

# ==============================================
# Testing
# ==============================================

# Test mode (set to true to use mock data)
TEST_MODE=false

# Test database
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/test_dbname

# ==============================================
# Notes
# ==============================================
# 1. Replace all placeholder values with your actual credentials
# 2. Never commit the .env file to version control
# 3. Add .env to your .gitignore file
# 4. Use different values for development, staging, and production
# 5. For production, use a secret manager (AWS Secrets Manager, HashiCorp Vault, etc.)
# 6. Some variables are optional and only needed if you use those services
# 7. API keys shown here are examples and won't work
